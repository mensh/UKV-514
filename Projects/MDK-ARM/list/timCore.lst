N  * This parameter can be:
N  *   @arg PWR_EXTI_LINE_PVD     
N  * @retval None.
N  */
N#define __HAL_PVD_EXTI_ENABLE_IT(__EXTILINE__)   (EXTI->IMR |= (__EXTILINE__))
N
N/**
N  * @brief Disable the PVD EXTI Line.
N  * @param  __EXTILINE__: specifies the PVD EXTI sources to be disabled.
N  * This parameter can be:
N  *  @arg PWR_EXTI_LINE_PVD    
N  * @retval None.
N  */
N#define __HAL_PVD_EXTI_DISABLE_IT(__EXTILINE__)  (EXTI->IMR &= ~(__EXTILINE__))
N
